<h1>List#index</h1>
<p>Find me in app/views/list/index.html.erb</p>
<%= link_to "Create a list", new_list_path%>

    <% @lists.each do |list| %>
      <div class="card-category p-5 m-4" style="width: 450px">
      <li class="list-unstyled">
      <%= link_to "Delete the list", list_path(list),
  data: {turbo_method: :delete, turbo_confirm: "Are you sure?"}
%>
      <% if list.photo.attached? %>
        <div class="card-category" style="background-image: linear-gradient(rgba(0,0,0,0.3), rgba(0,0,0,0.3)), url(<%= cl_image_path list.photo.key%>)">
      <%end %>
      <h4><%= link_to list.name, list_path(list) %></li></h4>
</div>
  <%end %>
  </div>
</div>
